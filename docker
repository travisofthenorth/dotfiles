alias dc='docker-compose'
alias drm_images='docker images -q | xargs docker rmi'
alias drm_dangling_images='docker images -f dangling=true -q | xargs docker rmi'
alias drm_containers='docker ps -aq | xargs docker rm'
alias drm_volumes='docker volume ls -q | xargs docker volume rm'
alias drm_stopped_containers='docker ps -aq --no-trunc -f status=exited | xargs docker rm'
